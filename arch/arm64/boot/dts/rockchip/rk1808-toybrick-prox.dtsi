#include <dt-bindings/display/drm_mipi_dsi.h>
#include <dt-bindings/display/media-bus-format.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/sensor-dev.h>
#include "rk1808.dtsi"

/ {
	model = "Rockchip RK1808 Compute stick";
	compatible = "rockchip,rk1808-compute", "rockchip,rk1808";

	fiq-debugger {
		compatible = "rockchip,fiq-debugger";
		rockchip,serial-id = <2>;
		rockchip,wake-irq = <0>;
		/* If enable uart uses irq instead of fiq */
		rockchip,irq-mode-enable = <0>;
		rockchip,baudrate = <1500000>;  /* Only 115200 and 1500000 */
		interrupts = <GIC_SPI 242 IRQ_TYPE_LEVEL_HIGH>;
		status = "okay";
	};

	keys: gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <&volup_key>;

		vol-up {
			gpios = <&gpio0 RK_PC7 GPIO_ACTIVE_HIGH>;
			label = "volume up";
			linux,code = <115>;
		};
	};


	firmware {
		optee {
			compatible = "linaro,optee-tz";
			method = "smc";
		};
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		drm_logo: drm-logo@00000000 {
			compatible = "rockchip,drm-logo";
			reg = <0x0 0x0 0x0 0x0>;
		};

		ramoops_mem: region@110000 {
			reg = <0x0 0x110000 0x0 0xf0000>;
		};
	};

	ramoops: ramoops {
		compatible = "ramoops";
		record-size = <0x0 0x30000>;
		console-size = <0x0 0xc0000>;
		ftrace-size = <0x0 0x00000>;
		pmsg-size = <0x0 0x00000>;
		memory-region = <&ramoops_mem>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
	};

	vdd_log: vdd-log {
		compatible = "regulator-fixed";
		regulator-name = "vdd_log";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <800000>;
		regulator-max-microvolt = <800000>;
	};

	vdd_cpu: vdd-cpu {
		compatible = "regulator-fixed";
		regulator-name = "vdd_cpu";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <850000>;
		regulator-max-microvolt = <850000>;
	};
};

&i2c1 {
	status = "okay";

	vdd_npu: tcs452x@1c {
		compatible = "tcs,tcs452x";
		reg = <0x1c>;
		vin-supply = <&vcc5v0_sys>;
		regulator-compatible = "fan53555-reg";
		pinctrl-0 = <&vsel_gpio>;
		vsel-gpios = <&gpio0 RK_PC6 GPIO_ACTIVE_HIGH>;
		regulator-name = "vdd_npu";
		regulator-min-microvolt = <750000>;
		regulator-max-microvolt = <900000>;
		regulator-ramp-delay = <2300>;
		fcs,suspend-voltage-selector = <1>;
		regulator-always-on;
		regulator-boot-on;
		regulator-initial-state = <3>;
		regulator-state-mem {
			regulator-off-in-suspend;
		};
	};

	vcamera@30 {
		compatible = "rockchip,virtual-camera";
		reg = <0x30>;
		width = <1280>;
		height = <720>;
		bus-format = <MEDIA_BUS_FMT_UYVY8_2X8>;

		port {
			vcamera_out: endpoint {
				remote-endpoint = <&mipi_in_ucam>;
				link-frequencies = /bits/ 64 <320000000>;
			};
		};
	};
};

&cpu0 {
	cpu-supply = <&vdd_cpu>;
};

&cpu1 {
	cpu-supply = <&vdd_cpu>;
};

&dfi {
	status = "okay";
};

&dmc {
	status = "okay";
	center-supply = <&vdd_log>;
};

&dmc_opp_table {

	opp-924000000 {
		opp-hz = /bits/ 64 <924000000>;
		opp-microvolt = <750000>;
		status = "disabled";
	};
};

&combphy {
	status = "okay";
};

&vpu_service {
	status = "okay";
};

&vpu_mmu {
	status = "okay";
};

&emmc {
	bus-width = <8>;
	cap-mmc-highspeed;
	max-frequency = <200000000>;
	mmc-hs200-1_8v;
	supports-emmc;
	non-removable;
	num-slots = <1>;
	status = "okay";
};

&npu {
	npu-supply = <&vdd_npu>;
	status = "okay";
};

&power {
	pd_npu-supply = <&vdd_npu>;
};

&sfc {
	status = "okay";
};

&u2phy {
	status = "okay";
};

&u2phy_host {
	status = "okay";
};

&u2phy_otg {
	rockchip,vbus-always-on;
	status = "okay";
};

&usb_host0_ehci {
	status = "okay";
};

&usb_host0_ohci {
	status = "okay";
};

&usbdrd3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&usb3_host_en>;
};

&usbdrd_dwc3 {
	dr_mode = "peripheral";
	snps,tx-fifo-resize;
	status = "okay";
};

&rk_rga {
	status = "okay";
};

&mipi_dphy {
        status = "okay";
};

&pinctrl {

	vsel_gpio: vsel-gpio {
		rockchip,pins =
			<0 RK_PC6 0 &pcfg_pull_down>;
	};

	volup_key: volup-key {
		rockchip,pins =
			<0 RK_PC7 RK_FUNC_GPIO &pcfg_output_low>;
	};
	usb3 {
        usb3_host_en: usb3-host-en {
            rockchip,pins =
                <1 RK_PB5 RK_FUNC_GPIO &pcfg_output_high>;
        };
    };
};
