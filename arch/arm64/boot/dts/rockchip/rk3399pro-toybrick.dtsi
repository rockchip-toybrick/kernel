			rockchip,pins =
				<1 RK_PA2 0 &pcfg_pull_up>;
			};
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins =
				<0 RK_PB5 0 &pcfg_pull_up>;
		};
	};

	lcd_rst {
