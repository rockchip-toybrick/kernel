		};

		mini_pcie_pins: mini-pcie-pins {
			rockchip,pins =
				<2 26 RK_FUNC_GPIO &pcfg_output_low>,
				<2 27 RK_FUNC_GPIO &pcfg_output_low>;
		};
	};
